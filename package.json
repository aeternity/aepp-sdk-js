{
  "name": "@aeternity/aepp-sdk",
  "version": "14.0.0",
  "description": "SDK for the Ã¦ternity blockchain",
  "main": "dist/aepp-sdk.cjs",
  "types": "es/index.d.ts",
  "module": "es/index.js",
  "type": "module",
  "browser": {
    "es/index.js": "./es/index-browser.js",
    "dist/aepp-sdk.cjs": "./dist/aepp-sdk.browser.cjs"
  },
  "exports": {
    "node": {
      "types": "./es/index.d.ts",
      "import": "./es/index.js",
      "default": "./dist/aepp-sdk.cjs"
    },
    "types": "./es/index-browser.d.ts",
    "import": "./es/index-browser.js",
    "default": "./dist/aepp-sdk.browser.cjs"
  },
  "sideEffects": false,
  "scripts": {
    "build:assets": "node tooling/fetch-aesophia-cli.js",
    "build:types": "tsc",
    "build:dist": "webpack",
    "build:es": "babel src --config-file ./babel.esm.config.js --out-dir es --extensions .ts --source-maps true",
    "build:api:node": "autorest tooling/autorest/node.yaml && node tooling/autorest/postprocessing.js node",
    "build:api:compiler": "node tooling/autorest/compiler-prepare.js && autorest tooling/autorest/compiler.yaml && node tooling/autorest/postprocessing.js compiler",
    "rem:build:api:middleware": "node tooling/autorest/middleware-prepare.js",
    "build:api:middleware": "autorest tooling/autorest/middleware.yaml && node tooling/autorest/postprocessing.js middleware",
    "build:generate": "tsx tooling/generate-schema.ts",
    "build": "run-p build:api:* build:assets build:generate && run-p build:dist build:es build:types",
    "docs:examples": "node tooling/docs/examples-to-md.js examples/node/*.js",
    "docs:api": "typedoc",
    "commitlint": "commitlint --from develop",
    "lint": "run-p lint:*",
    "lint:prettier": "prettier . --check",
    "lint:eslint": "eslint . --ext .ts,.js",
    "lint:types": "tsc -p tsconfig.tests.json",
    "format": "prettier . --write",
    "test": "mocha './test/unit/' './test/integration/'",
    "test:assets": "node tooling/fetch-metamask.js",
    "test:integration": "mocha './test/integration/'",
    "test:unit": "mocha './test/unit/'",
    "test:watch": "mocha './test/unit/' './test/integration/' --watch",
    "prepare": "npm run build",
    "prepublishOnly": "test/examples.sh",
    "release": "standard-version --skip.tag --infile docs/CHANGELOG.md",
    "release-alpha": "npm run release -- --prerelease alpha",
    "release-beta": "npm run release -- --prerelease beta"
  },
  "license": "ISC",
  "keywords": [
    "API",
    "aeternity",
    "blockchain",
    "SDK"
  ],
  "dependencies": {
    "@aeternity/aepp-calldata": "^1.8.0",
    "@azure/core-client": "^1.9.2",
    "@azure/core-rest-pipeline": "^1.17.0",
    "@babel/runtime-corejs3": "^7.25.7",
    "@ledgerhq/hw-transport": "^6.31.4",
    "@metamask/providers": "17.2.0 || ^17.2.2",
    "@scure/bip39": "^1.4.0",
    "@types/json-bigint": "^1.0.4",
    "@types/node": "^22.7.5",
    "@types/readable-stream": "^4.0.15",
    "@types/sha.js": "^2.4.4",
    "@types/webextension-polyfill": "^0.12.1",
    "@types/websocket": "^1.0.10",
    "@types/ws": "^8.5.12",
    "bignumber.js": "^9.1.2",
    "blakejs": "^1.2.1",
    "bs58": "^6.0.0",
    "buffer": "^6.0.3",
    "canonicalize": "^2.0.0",
    "eventemitter3": "^5.0.1",
    "events": "^3.3.0",
    "isomorphic-ws": "^5.0.0",
    "json-bigint": "^1.0.0",
    "process": "^0.11.10",
    "rlp": "^3.0.0",
    "sha.js": "^2.4.11",
    "tweetnacl": "^1.0.3",
    "tweetnacl-auth": "^1.0.1",
    "varuint-bitcoin": "^2.0.0",
    "webextension-polyfill": "^0.12.0",
    "websocket": "^1.0.35",
    "ws": "^8.18.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aeternity/aepp-sdk-js.git"
  },
  "devDependencies": {
    "@babel/cli": "^7.25.7",
    "@babel/core": "^7.25.8",
    "@babel/node": "^7.25.7",
    "@babel/plugin-transform-runtime": "^7.25.7",
    "@babel/preset-env": "^7.25.8",
    "@babel/preset-typescript": "^7.25.7",
    "@babel/register": "^7.25.7",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@ledgerhq/hw-transport-mocker": "^6.29.4",
    "@ledgerhq/hw-transport-node-hid-singleton": "^6.31.5",
    "@ledgerhq/hw-transport-webusb": "^6.29.4",
    "@types/chai": "^5.0.0",
    "@types/chai-as-promised": "^8.0.1",
    "@types/mocha": "^10.0.9",
    "@types/node-hid": "^1.3.4",
    "@types/sinon": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "autorest": "^3.7.1",
    "babel-loader": "^9.2.1",
    "babel-plugin-import-globals": "^2.0.0",
    "babel-plugin-transform-import-meta": "^2.2.1",
    "chai": "^5.1.1",
    "chai-as-promised": "^8.0.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-rulesdir": "^0.2.2",
    "eslint-plugin-tsdoc": "^0.3.0",
    "extract-zip": "^2.0.1",
    "html-webpack-plugin": "^5.6.0",
    "mocha": "^10.7.3",
    "npm-run-all2": "^6.2.3",
    "nyc": "^17.1.0",
    "prettier": "^3.3.3",
    "puppeteer-core": "^23.5.3",
    "sinon": "^19.0.2",
    "source-map": "^0.7.4",
    "standard-version": "^9.5.0",
    "tsx": "^4.19.1",
    "typedoc": "^0.26.10",
    "typedoc-plugin-missing-exports": "^3.0.0",
    "typescript": "^5.6.3",
    "webpack": "^5.95.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4"
  },
  "files": [
    "dist",
    "bin",
    "es"
  ],
  "browserslist": {
    "node": [
      "node >= 18.19"
    ],
    "browser": [
      "> 1%",
      "last 2 versions",
      "not dead"
    ],
    "production": [
      "node >= 18.19",
      "> 1%",
      "last 2 versions",
      "not dead"
    ]
  },
  "engines": {
    "node": ">=18.19"
  },
  "packageManager": "npm@10.9.0",
  "contributors": [
    "Till Kolter <till.kolter@gmail.com>",
    "Alexander Kahl <ak@sodosopa.io>",
    "Nazar Duchak <omelko39@gmail.com>",
    "Denis Davidyuk <denis_davidyuk@hotmail.com>"
  ]
}
