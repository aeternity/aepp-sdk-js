{
  "name": "@aeternity/aepp-sdk",
  "version": "7.5.0",
  "description": "SDK for the Ã¦ternity blockchain",
  "main": "dist/aepp-sdk.js",
  "browser": "dist/aepp-sdk.browser.js",
  "module": "es/index.js",
  "sideEffects": false,
  "scripts": {
    "build": "webpack -p",
    "build:dev": "webpack --mode=development",
    "docs:docco": "docco -t tooling/docs/markdown.jst -c /dev/null examples/node/*.js && rm -f docs/null && node rename-to-md.js",
    "docs:api": "node apidoc.js",
    "test": "nyc --require @babel/register mocha --recursive --exit",
    "report-coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "test-channel": "nyc --require @babel/register mocha \"./test/integration/channel.js\" --exit",
    "test-aens": "nyc --require @babel/register mocha \"./test/integration/aens.js\"",
    "test-else": "nyc --require @babel/register mocha \"./test/**/!(aens|channel).js\"",
    "test:watch": "mocha --recursive --require @babel/register --watch",
    "prepare": "npm run build",
    "genChangelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "prepublishOnly": "npm run docs:docco && npm run docs:api"
  },
  "license": "ISC",
  "keywords": [
    "API",
    "aeternity",
    "blockchain",
    "SDK"
  ],
  "dependencies": {
    "@aeternity/bip39": "^0.1.0",
    "@stamp/it": "^1.0.3",
    "@stamp/required": "^1.0.1",
    "aes-js": "^3.1.1",
    "axios": "^0.19.0",
    "bignumber.js": "^9.0.0",
    "bip32-path": "^0.4.2",
    "blakejs": "^1.1.0",
    "bs58check": "^2.1.1",
    "ed2curve": "^0.3.0",
    "joi-browser": "^13.4.0",
    "libsodium-wrappers-sumo": "0.7.6",
    "ramda": "^0.27.0",
    "rlp": "2.2.5",
    "sha.js": "^2.4.11",
    "tweetnacl": "^1.0.0",
    "tweetnacl-auth": "^1.0.1",
    "uuid": "^8.0.0",
    "websocket": "^1.0.26"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aeternity/aepp-sdk-js"
  },
  "devDependencies": {
    "@babel/cli": "7.10.1",
    "@babel/core": "7.10.2",
    "@babel/node": "7.10.1",
    "@babel/plugin-proposal-export-default-from": "7.10.1",
    "@babel/plugin-proposal-object-rest-spread": "7.10.1",
    "@babel/plugin-transform-runtime": "7.10.1",
    "@babel/preset-env": "7.10.2",
    "@babel/register": "7.10.1",
    "@babel/runtime": "^7.10.2",
    "babel-loader": "^8.0.0-beta.2",
    "babel-plugin-ramda": "^2.0.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "codecov": "^3.6.1",
    "commander": "^5.0.0",
    "cz-conventional-changelog": "^3.0.0",
    "docco": "^0.8.0",
    "eslint": "^6.0.1",
    "eslint-config-standard": "^14.0.0",
    "eslint-import-resolver-node": "^0.3.2",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "jsdoc-to-markdown": "^6.0.1",
    "mocha": "^7.0.1",
    "nyc": "^15.0.0",
    "recursive-rename": "^4.0.0",
    "sinon": "^9.0.0",
    "source-map": "^0.7.3",
    "standard": "^14.0.1",
    "standard-loader": "^7.0.0",
    "webpack": "^4.36.1",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cli": "3.3.8"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
