    "bignumber.js": "^9.0.0",
    "bip32-path": "^0.4.2",
    "blakejs": "^1.1.0",
    "bs58check": "^2.1.1",
    "ed2curve": "^0.3.0",
    "joi-browser": "^13.4.0",
    "libsodium-wrappers-sumo": "0.7.6",
    "ramda": "^0.26.1",
    "rlp": "2.2.4",
    "serialize-javascript": "^2.1.2",
    "sha.js": "^2.4.11",
    "tweetnacl": "^1.0.0",
    "tweetnacl-auth": "^1.0.1",
    "uuid": "^3.3.2",
    "websocket": "^1.0.26"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aeternity/aepp-sdk-js"
